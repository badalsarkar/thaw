    <munder>
        <mo>&sum;</mo>
        <mfrac linethickness="0">
            <mrow>
                <mn>0</mn>
                <mo>&le;</mo>
                <mi>i</mi>
                <mo>&le;</mo>
                <mi>m</mi>
            </mrow>
            <mrow>
                <mn>0</mn>
                <mo>&lt;</mo>
                <mi>j</mi>
                <mo>&lt;</mo>
                <mi>n</mi>
            </mrow>
        </mfrac>
    </munder>
    <mi>P</mi>
    <mrow>
        <mo>(</mo>
        <mi>i</mi>
        <mo>,</mo>
        <mi>j</mi>
        <mo>)</mo>
    </mrow>
</math>
'#


