#H3, numbered=false# Limit

#IMAGE, src="img/stirling29.png", scale=false#

#MATH, '
<math>
    <munder>
        <mi>lim</mi>
        <mrow>
            <mi>n</mi>
            <mo>→</mo>
            <mo rspace="0">+</mo>
            <mi>∞</mi>
        </mrow>
    </munder>
    <mfrac>
        <msqrt>
            <mrow>
                <mn>2</mn>
                <mi>&pi;</mi>
                <mi>n</mi>
            </mrow>
        </msqrt>
        <mrow>
            <mn>n</mn>
            <mo>!</mo>
        </mrow>
    </mfrac>
    <msup>
        <mrow>
            <mo>(</mo>
            <mfrac>
                <mi>n</mi>
                <mi>e</mi>
            </mfrac>
            <mo>)</mo>
        </mrow>
        <mi>n</mi>
    </msup>
    <mo>=</mo>
    <mn>1</mn>
</math>
'#


