    <mi>ϵ</mi>
    <mrow>
        <mo>(</mo>
        <mi>&sigma;</mi>
        <mo>)</mo>
    </mrow>
    <munderover>
        <mo>&prod;</mo>
        <mrow>
            <mi>i</mi>
            <mo>=</mo>
            <mn>1</mn>
        </mrow>
        <mi>n</mi>
    </munderover>
    <msub>
        <mi>a</mi>
        <mrow>
            <mi>i</mi>
            <mo>,</mo>
            <msub>
                <mi>&sigma;</mi>
                <mi>i</mi>
