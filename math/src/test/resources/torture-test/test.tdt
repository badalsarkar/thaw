#H3, numbered=false# Complex equation

#IMAGE, src="img/ex34.png", scale=false#

#MATH, '
<math>
    <mrow>
        <mrow>
            <mo>(</mo>
            <mfrac>
                <msup>
                    <mo>∂</mo>
                    <mn>2</mn>
                </msup>
                <mrow>
                    <mo>∂</mo>
                    <msup>
                        <mi>x</mi>
                        <mn>2</mn>
                    </msup>
                </mrow>
            </mfrac>
            <mo>+</mo>
            <mfrac>
                <msup>
                    <mo>∂</mo>
                    <mn>2</mn>
                </msup>
                <mrow>
                    <mo>∂</mo>
                    <msup>
                        <mi>y</mi>
                        <mn>2</mn>
                    </msup>
                </mrow>
            </mfrac>
            <mo>)</mo>
        </mrow>
        <msup>
            <mrow>
                <mo>|</mo>
                <mi>φ</mi>
                <mo>(</mo>
                <mi>x</mi>
                <mo>+</mo>
                <mi>i</mi>
                <mi>y</mi>
                <mo>)</mo>
                <mo>|</mo>
            </mrow>
            <mn>2</mn>
        </msup>
        <mo>=</mo>
        <mn>0</mn>
    </mrow>
</math>
'#


